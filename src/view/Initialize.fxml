<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="182.0" prefWidth="516.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.InitializeController">
   <children>
      <Label layoutX="14.0" layoutY="35.0" text="Course Management Simulation System" textFill="#b3b83d">
         <font>
            <Font name="Gill Sans MT" size="31.0" />
         </font>
      </Label>
      <Button fx:id="resumeSim" layoutX="263.0" layoutY="106.0" mnemonicParsing="false" onAction="#handleResumePressed" text="Resume Simulation" />
      <Button fx:id="restartSim" layoutX="66.0" layoutY="106.0" mnemonicParsing="false" onAction="#handleRestartSimPressed" text="Start New Simulation" />
   </children>
</AnchorPane>
